        link(rel="stylesheet", href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.1/css/regular.min.css", integrity="sha512-rVrbAp27ffQyFnzJ/aC5fZv9JgvL6cdB4zsL5HmM+DhJdzThc/F//62SJF+CaGiOZTP35e1p8JGcc+zRRVuhRw==", crossorigin="anonymous")
        link(rel="stylesheet", href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.1/css/solid.min.css", integrity="sha512-SazV6tF6Ko4GxhyIZpKoXiKYndqzh7+cqxl9HDH7DGSpjkCN3QLqTAlhJHJnKxu3/2rfCsltzwFC4CmxZE1hPg==", crossorigin="anonymous")
        title Warlock-UI
    body
        header
            nav(class="px-4 py-2 navbar navbar-expand-md navbar-dark bg-primary")
                a(class="navbar-brand btn-sidenav",href="#",data-target="#sidenav") Warlock-UI
                button(class="btn btn-outline-light navbar-toggler fa fa-bars",data-toggle="collapse",data-target="#navbarSupportedContent")
                div(class="collapse navbar-collapse",id="navbarSupportedContent")
                    ul(class="mr-auto navbar-nav mb-lg-0")
                        li(class="nav-item")
                            a(class="nav-link",href="#") Link
        main
            div(class="sidenav side-dark bg-primary",id="sidenav")
                button(type="button",class="btn-close-side fa fa-times")
                div(class="side-header w-100")
                    div(class="background")
                        img(src="https://i.pinimg.com/originals/90/55/07/905507df95937e17cd98555c54d3bf98.jpg")
                    div(style="height:100px")
                div(class="side-body w-100")
                    ul
                        li
                            a(class="px-6 text-white link-url",data-url="alert.html",href="#") Alert
                        li
                            a(class="px-6 text-white link-url",data-url="avatar.html",href="#") Avatar
                        li
                            a(class="px-6 text-white link-url",data-url="badge.html",href="#") Badge
                        li
                            a(class="px-6 text-white link-url",data-url="bars.html",href="#") Bars
                        li
                            a(class="px-6 text-white link-url",data-url="breadcrumb.html",href="#") Breadcrumb
                        li
                            a(class="px-6 text-white link-url",data-url="buttons.html",href="#") Buttons
                        li
                            a(class="px-6 text-white link-url",data-url="cards.html",href="#") Cards
                        li  
                            a(class="px-6 text-white link-url",data-url="carousel.html",href="#") Carousel
                        li
                            a(class="px-6 text-white link-url",data-url="collapse.html",href="#") Collapse
                        li
                            a(class="px-6 text-white link-url",data-url="dropdowns.html",href="#") Dropdowns
                        li
                            a(class="px-6 text-white link-url dropdown-toggle",href="#",id="dropdownMenuForm") Forms
                            ul(class="dropdown-menu dropdown-relative",aria-labelledby="dropdownMenuForm")
                                li
                                    a(class="px-6 text-white link-url",data-url="form/input.html",href="#") Input
                                li
                                    a(class="px-6 text-white link-url",data-url="form/switch.html",href="#") Switch
                                li
                                    a(class="px-6 text-white link-url",data-url="form/check.html",href="#") Check
                                li
                                    a(class="px-6 text-white link-url",data-url="form/radio.html",href="#") Radio
                                li
                                    a(class="px-6 text-white link-url",data-url="form/range.html",href="#") Range
                                li
                                    a(class="px-6 text-white link-url",data-url="form/select.html",href="#") Select
                                li
                                    a(class="px-6 text-white link-url",data-url="form/chip.html",href="#") Chip
                        li
                            a(class="px-6 text-white link-url",data-url="modal.html",href="#") Modal
                        li
                            a(class="px-6 text-white link-url",data-url="nav.html",href="#") Nav
                        li
                            a(class="px-6 text-white link-url",data-url="navbar.html",href="#") Navbar
                        li
                            a(class="px-6 text-white link-url",data-url="pagination.html",href="#") Pagination
                        li
                            a(class="px-6 text-white link-url",data-url="sidenav.html",href="#") Sidenav
                        li
                            a(class="px-6 text-white link-url",data-url="spinners.html",href="#") Spinners
                        li
                            a(class="px-6 text-white link-url",data-url="tooltips.html",href="#") Tooltips
            div(id="render")
        footer
            div(class="bg-primary w-100",style="height:50px")
        script(src="js/warlock.js")
        script(src="https://cdnjs.cloudflare.com/ajax/libs/prettify/r298/prettify.min.js")
        script(src="js/main.js")
        script(src="https://cdnjs.cloudflare.com/ajax/libs/holder/2.9.7/holder.js")